# Terraform Variables for Assure360 Prerequisites
# Copy this file to .env and update with your actual values
# Terraform automatically reads TF_VAR_ prefixed environment variables

# AWS Configuration
TF_VAR_region="ap-southeast-2"
TF_VAR_aws_profile="your-aws-profile"

# Common tags (JSON format)
TF_VAR_common_tags='{
  "Project": "Assure360",
  "ManagedBy": "Terraform",
  "Environment": "production",
  "Owner": "Platform Team",
  "CostCenter": "ENG",
  "Team": "Platform"
}'

# Optional: Override group name (default: davidson-developers)
# TF_VAR_group_name="davidson-developers"

# Optional: Override policy names
# TF_VAR_power_user_policy_name="DavidsonPowerUserAccess"
# TF_VAR_restrictions_policy_name="DavidsonServiceRestrictions"
# TF_VAR_cross_account_role_name="DavidsonCrossAccountRole"

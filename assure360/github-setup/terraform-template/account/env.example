# Terraform Variables for Account Creation
# Copy this file to .env and update with your actual values
# Terraform automatically reads TF_VAR_ prefixed environment variables

# Account Configuration
TF_VAR_account_name="YOUR_PROJECT"
TF_VAR_account_email="your-email@yourdomain.com"

# Organization Settings
TF_VAR_parent_ou_id="ou-xxxxxxxxx"
TF_VAR_iam_user_access_to_billing="DENY"

# AWS Configuration
TF_VAR_aws_profile="your-aws-profile"
TF_VAR_aws_region="us-east-1"

# Tags (optional)
TF_VAR_tags='{"Environment": "production", "Owner": "your-team", "Project": "YOUR_PROJECT"}'

# Terraform Variables for User Creation
# Copy this file to .env and update with your actual values
# Terraform automatically reads TF_VAR_ prefixed environment variables

# Account Configuration
TF_VAR_member_account_id="123456789012"
TF_VAR_management_region="us-east-1"
TF_VAR_new_account_region="ap-southeast-2"
TF_VAR_bootstrap_role_name="OrganizationAccountAccessRole"

# User Configuration
TF_VAR_developer_user_name="your-dev-user"
TF_VAR_admin_user_name="your-admin-user"
TF_VAR_monitor_user_name="your-monitor-user"

# Enable/disable specific users
TF_VAR_enable_developer_user="true"
TF_VAR_enable_admin_user="true"
TF_VAR_enable_monitor_user="true"

# AWS Configuration
TF_VAR_aws_profile="your-aws-profile"

# Common tags (JSON format)
TF_VAR_tags='{
  "Project": "YOUR_PROJECT",
  "ManagedBy": "Terraform",
  "Environment": "multi-account",
  "Owner": "DevOps Team"
}'

# Terraform Variables for Parameter Store
# Copy this file to .env and update with your actual values
# Terraform automatically reads TF_VAR_ prefixed environment variables

# Account Configuration
TF_VAR_member_account_id="123456789012"
TF_VAR_management_region="us-east-1"
TF_VAR_member_account_region="ap-southeast-2"
TF_VAR_bootstrap_role_name="OrganizationAccountAccessRole"
TF_VAR_developer_username="your-dev-user"

# Service Credentials (JSON format)
TF_VAR_secrets='{
  "service_a": {
    "access_key": "your-service-a-access-key-here",
    "client_key": "your-service-a-client-key-here"
  },
  "service_b_store1": {
    "store_name": "your-store1.example.com",
    "access_token": "your-service-b-access-token-1"
  },
  "service_b_store2": {
    "store_name": "your-store2.example.com", 
    "access_token": "your-service-b-access-token-2"
  },
  "service_c": {
    "client_id": "your-service-c-client-id-here",
    "client_secret": "your-service-c-client-secret-here",
    "access_token": "your-service-c-access-token-here",
    "refresh_token": "your-service-c-refresh-token-here",
    "api_domain": "https://api.service-c.example.com",
    "expires_in": 3600
  }
}'

# Configuration
TF_VAR_parameter_prefix="/secret/"
TF_VAR_environment="production"
TF_VAR_aws_profile="your-aws-profile"
